INSERT INTO `locales_source` VALUES ('8522','/admin/config/system/backup_migrate?render=overlay','default','Save a copy to a second destination','','7.38'),('8523','/admin/config/system/backup_migrate?render=overlay','default','Second Backup Destination','','7.38'),('8524','/admin/config/system/backup_migrate?render=overlay','default','Note','','7.38'),('8525','/admin/config/system/backup_migrate?render=overlay','default','This note will be saved with the backup file and shown on the listing page.','','7.38'),('8526','/admin/config/system/backup_migrate?render=overlay','default','Save a local copy to','','7.38'),('8527','sites/all/modules/ckeditor/includes/ckeditor.utils.js','default','Content can only be inserted into CKEditor in the WYSIWYG mode.','','none'),('8528','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Below is a list of security filters along with their status for each text format.<br /><br />The following statuses are available: !img_enabled - the filter is configured for a given text format and will be run on the content during the filtering process. !img_disabled - the filter is disabled for a given text format and will not be run on the content during the filtering process.<br /><br />In order to modify the security filters settings for each of the text formats, you need to visit the text format configuration sections as listed below: !elem_format_config','','7.38'),('8529','/admin/config/content/ckeditor/edit/Full?render=overlay','default','!title <a href="!link" target="_blank">project page</a>','','7.38'),('8530','/admin/config/content/ckeditor/edit/Full?render=overlay','default','@data','','7.38'),('8531','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Current toolbar','','7.38'),('8532','/admin/config/content/ckeditor/edit/Full?render=overlay','default','DrupalBreak','','7.38'),('8533','/admin/config/content/ckeditor/edit/Full?render=overlay','default','MediaEmbed','','7.38'),('8534','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Default editor state','','7.38');
INSERT INTO `locales_source` VALUES ('8535','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Choose the language direction used in the editing area. Even when CKEditor automatically detects the user language and adjusts its user interface, the editing area is not automatically changed into the LTR or RTL mode. To be able to type LTR (like English) and RTL (like Arabic, Hebrew, Persian) content at the same time, please make sure that the <strong>!bidiltr</strong> and <strong>!bidirtl</strong> buttons are enabled in the toolbar.','','7.38'),('8536','/admin/config/content/ckeditor/edit/Full?render=overlay','default','this blog post','','7.38'),('8537','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Configure input data filtering in CKEditor. See !link for more information about ACF.','','7.38'),('8538','/admin/config/content/ckeditor/edit/Full?render=overlay','default','<strong>Example:</strong> if only Bold and Link buttons are enabled, only !strong and !a tags will be allowed. Any !b tags will automatically be converted into !strong. The !img tag will not be allowed, unless the Image button is added to the toolbar.','','7.38'),('8539','/admin/config/content/ckeditor/edit/Full?render=overlay','default','<strong>Warning:</strong> CKEditor will not use the set of tags associated with text format. Only the buttons enabled in the toolbar and optional plugins will determine which tags/attributes/styles are handled by CKEditor. Enabling ACF in CKEditor does not affect server-side filtering done by Drupal in any way.','','7.38'),('8540','/admin/config/content/ckeditor/edit/Full?render=overlay','default','CKEditor will only allow tags/attributes/styles provided by CKEditor features. Disallowed content will be removed directly by CKEditor. When configured properly, it helps to ensure a true WYSIWYG experience.','','7.38'),('8541','/admin/config/content/ckeditor/edit/Full?render=overlay','default','CKEditor will not filter any tags and as a result, the user will be able to insert any content into the editor. The content will only be filtered by server-side filters associated with input formats.','','7.38');
INSERT INTO `locales_source` VALUES ('8542','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Extra allowed content','','7.38'),('8543','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Set additional allowed content rules for CKEditor. Separate rules with a semicolon. See !docs for more information.','','7.38'),('8544','/admin/config/content/ckeditor/edit/Full?render=overlay','default','<strong>Example:</strong> !rules','','7.38'),('8545','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Semicolon-separated list of HTML font formats. Allowed values are: !allowed_values','','7.38'),('8546','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Defines the CSS to be used in the editor area.!title_theme_css &ndash; load the !style_css file from the current site theme.!title_define_css &ndash; enter the CSS file path below.!title_ckeditor_default &ndash; use the default editor CSS.','','7.38'),('8547','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Enter the path to the CSS file (Example: !example1) or a list of CSS files separated with a comma (Example: !example2). Make sure you select the !title_define_css option above.','','7.38'),('8548','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Available placeholders:!h &ndash; host name (!host).!t &ndash; path to theme (!theme).','','7.38'),('8549','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Define the location of the !ckeditor_styles_js_file file. It is used by the <strong>Style</strong> drop-down list available in the default toolbar. Copy the !ckeditor_styles_js_path file into your theme directory (!theme) and adjust it to your needs.','','7.38'),('8550','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Enter the path to a file with predefined styles (Example: !example1). Make sure you select the !style_define_path option above.','','7.38'),('8551','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Available placeholders:!h &ndash; host name (!host).!t &ndash; path to theme (!theme).!m &ndash; path to the CKEditor module (!module).','','7.38');
INSERT INTO `locales_source` VALUES ('8552','/admin/config/content/ckeditor/edit/Full?render=overlay','default','elFinder','','7.38'),('8553','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Set the file browser settings. A file browser will allow you to browse the files stored on the server and embed them as links, images, or Flash movies. CKEditor is compatible with such Drupal modules as !imce or !elfinder. CKEditor can be also integrated with !ckfinder, an advanced Ajax file manager.','','7.38'),('8554','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Available placeholders:!b &ndash; the base URL path of the Drupal installation (!base).!f &ndash; the Drupal file system path where the files are stored (!files).!u &ndash; User ID.!n &ndash; Username.','','7.38'),('8555','/admin/config/content/ckeditor/edit/Full?render=overlay','default','Available placeholders:!d &ndash; the server path to the document root (!root).!b &ndash; the base URL path of the Drupal installation (!base).!f &ndash; the Drupal file system path where the files are stored (!files).!u &ndash; User ID.!n &ndash; Username.','','7.38'),('8556','/admin/config/content/ckeditor/edit/Full?render=overlay','default','In order to change CKEditor configuration globally, you should modify the !ckeditor_config configuration file. Sometimes it is required to change the CKEditor configuration for a single profile only. Use this box to define settings that are unique for this profile. Available options are listed in the !docs. Add the following code snippet to change the fonts available in the CKEditor <strong>Font</strong> and <strong>Size</strong> drop-down lists: <pre>@code</pre><strong>Warning</strong>: If you make a mistake here, CKEditor may not load correctly.','','7.38');
DROP TABLE IF EXISTS `locales_target`;
CREATE TABLE `locales_target` (   `lid` int(11) NOT NULL DEFAULT '0' COMMENT 'Source string ID. References locales_source.lid.',   `translation` blob NOT NULL COMMENT 'Translation string value in this language.',   `language` varchar(12) NOT NULL DEFAULT '' COMMENT 'Language code. References languages.language.',   `plid` int(11) NOT NULL DEFAULT '0' COMMENT 'Parent lid (lid of the previous string in the plural chain) in case of plural strings. References locales_source.lid.',   `plural` int(11) NOT NULL DEFAULT '0' COMMENT 'Plural index number in case of plural strings.',   `l10n_status` int(11) NOT NULL DEFAULT '0',   PRIMARY KEY (`language`,`lid`,`plural`),   KEY `lid` (`lid`),   KEY `plid` (`plid`),   KEY `plural` (`plural`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Stores translated versions of strings.';
