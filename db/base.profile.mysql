INSERT INTO `locales_source` VALUES ('4357','','default','The file %file could not be copied because a file by that name already exists in the destination directory.','','none'),('4358','','default','The specified file %file could not be moved, because the destination is invalid. More information is available in the system log.','','none'),('4359','','default','The specified file %file could not be deleted, because it is not a valid URI. More information is available in the system log.','','none'),('4360','','default','The data could not be saved, because the destination is invalid. More information is available in the system log.','','none'),('4361','','default','File %file (%realpath) could not be copied, because the destination %destination is invalid. This is often caused by improper use of file_copy() or a missing stream wrapper.','','none'),('4362','','default','File %file (%realpath) could not be copied because it does not exist.','','none'),('4363','','default','File %file could not be copied because a file by that name already exists in the destination directory (%directory)','','none'),('4364','','default','File %file could not be copied because it would overwrite itself.','','none'),('4365','','default','The specified file %file could not be copied to %destination.','','none'),('4366','','default','File %file (%realpath) could not be moved, because the destination %destination is invalid. This may be caused by improper use of file_move() or a missing stream wrapper.','','none'),('4367','','default','File %file (%realpath) could not be deleted because it is not a valid URI. This may be caused by improper use of file_delete() or a missing stream wrapper.','','none'),('4368','','default','The data could not be saved because the destination %destination is invalid. This may be caused by improper use of file_save_data() or a missing stream wrapper.','','none'),('4369','','default','Your PHP configuration only supports a single database type, so it has been aut