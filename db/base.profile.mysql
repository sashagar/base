INSERT INTO `locales_source` VALUES ('3207','','default','!name must be a hexadecimal color value.','','none'),('3208','','default','Width and height can not both be blank.','','none'),('3209','','default','The part of the image that will be retained during the crop.','','none'),('3210','','default','The background color to use for exposed areas of the image. Use web-style hex colors (#FFFFFF for white, #000000 for black). Leave blank for transparency on image types that support it.','','none'),('3211','','default','There are currently no styles. <a href="!url">Add a new one</a>.','','none'),('3212','','default','There are currently no effects in this style. Add one by selecting an option below.','','none'),('3213','','default','view actual size','','none'),('3214','','default','Sample original image','','none'),('3215','','default','Sample modified image','','none'),('3216','','default','Resizing will make images an exact set of dimensions. This may cause images to be stretched or shrunk disproportionately.','','none'),('3217','','default','Scaling will maintain the aspect-ratio of the original image. If only a single dimension is specified, the other dimension will be calculated.','','none'),('3218','','default','Scale and crop','','none'),('3219','','default','Scale and crop will maintain the aspect-ratio of the original image, then crop the larger dimension. This is most useful for creating perfectly square thumbnails without stretching the image.','','none'),('3220','','default','Cropping will remove portions of an image to make it the specified dimensions.','','none'),('3221','','default','Desaturate','','none'),('3222','','default','Desaturate converts an image to grayscale.','','none'),('3223','','default','Rotating an image may cause the dimensions of an image to increase to fit the diagonal.','','none'),('3224','','default','Image resize failed using the %toolkit toolkit on %path (%mimetype, %dimensions)','','none'),('3225','','default','Image scale failed