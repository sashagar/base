INSERT INTO `locales_source` VALUES ('4330','','default','@count[2] e-mails were sent during this test.','','none'),('4331','','default','Blocked IP addresses','','none'),('4332','','default','IP addresses listed here are blocked from your site. Blocked addresses are completely forbidden from accessing the site and instead see a brief message explaining the situation.','','none'),('4333','','default','The content type %rowtype had uploads disabled but contained uploaded file data. Uploads have been re-enabled to migrate the existing data. You may delete the "File attachments" field in the %rowtype type if this data is not necessary.','','none'),('4334','','default','Upload module has been migrated to File module.','','none'),('4335','','default','Available variables are: [site:name], [site:url], [user:name], [user:mail], [site:login-url], [site:url-brief], [user:edit-url], [user:one-time-login-url], [user:cancel-url].','','none'),('4336','','default','not yet assigned','','none'),('4337','','default','not yet created','','none'),('4338','','default','[user:name],\n\nThank you for registering at [site:name]. You may now log in by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password.\n\nAfter setting your password, you will be able to log in at [site:login-url] in the future using:\n\nusername: [user:name]\npassword: Your password\n\n--  [site:name] team','','none'),('4339','','default','[user:name],\n\nA site administrator at [site:name] has created an account for you. You may now log in by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password.\n\nAfter setting your password, you will be able to log in at [site:login-url] in the future using:\n\nusername: [user:name]\npassword: Your password\n\n--  [site:name] team','','none'),('4340','','default','[user:name],\n\nA request to reset the password for your account has been made at [site:name].\n\nYou may now log in by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password. It expires after one day and nothing will happen if it''s not used.\n\n--  [site:name] team','','none');
INSERT INTO `locales_source` VALUES ('4341','','default','[user:name],\n\nYour account on [site:name] has been blocked.\n\n--  [site:name] team','','none'),('4342','','default','[user:name],\n\nA request to cancel your account has been made at [site:name].\n\nYou may now cancel your account on [site:url-brief] by clicking this link or copying and pasting it into your browser:\n\n[user:cancel-url]\n\nNOTE: The cancellation of your account is not reversible.\n\nThis link expires in one day and nothing will happen if it is not used.\n\n--  [site:name] team','','none'),('4343','','default','[user:name],\n\nYour account on [site:name] has been canceled.\n\n--  [site:name] team','','none'),('4344','','default','The ability to send users their passwords in plain text has been removed in Drupal 7. Your existing email templates have been modified to remove it. You should <a href="@template-url">review these templates</a> to make sure they read properly.','','none'),('4345','','default','None (original image)','','none'),('4346','','default','The URL of the account edit page.','','7.38'),('4347','','default','Nonce from @endpoint rejected because it is not correctly formatted, nonce: @nonce.','','none'),('4348','','default','Nonce received from @endpoint is out of range (time difference: @intervals). Check possible clock skew.','','none'),('4349','','default','Nonce replay attempt blocked from @ip, nonce: @nonce.','','none'),('4350','','default','Comment display','','none'),('4351','','default','New field name','','none'),('4352','','default','Error opening socket @socket','','none'),('4353','','default','delivery callback not found','','none'),('4354','','default','callback %callback not found: %q.','','none'),('4355','','default','The specified file %file could not be copied, because the destination is invalid. More information is available in the system log.','','none'),('4356','','default','The specified file %file could not be copied, because the destination directory is