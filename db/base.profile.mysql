INSERT INTO `locales_source` VALUES ('4111','','default','A multi-column theme which can be configured to modify colors and switch between fixed and fluid width layouts.','','none'),('4112','','default','A simple one-column, tableless, fluid width administration theme.','','none'),('4113','','default','This theme demonstrates Drupal''s default HTML markup and CSS styles. To learn how to build your own theme and override Drupal''s default code, see the <a href="http://drupal.org/theme-guide">Theming Guide</a>.','','none'),('4114','','default','Image style','','none'),('4115','','default','1 pending update (@number_applied to be applied, @number_incompatible skipped)','','none'),('4116','','default','@count pending updates (@number_applied to be applied, @number_incompatible skipped)','','none'),('4117','','default','@count[2] pending updates (@number_applied to be applied, @number_incompatible skipped)','','none'),('4118','','default','The Blog module allows registered users to maintain an online journal, or <em>blog</em>. Blogs are made up of individual <em>blog entries</em>. By default, the blog entries are displayed by creation time in descending order, with comments enabled, and are promoted to the site''s front page. For more information, see the online handbook entry for <a href=''@blog''>Blog module</a>.','','none'),('4119','','default','Author textfield','','none'),('4120','','default','Subject textfield','','none'),('4121','','default','A contact form with category %category already exists.','','none'),('4122','','default','Hello !recipient-name,','','none'),('4123','','default','Attempt to create an instance of a field @field_name that doesn''t exist or is currently inactive.','','none'),('4124','','default','Attempt to create an instance of field @field_name on bundle @bundle that already has an instance of that field.','','none'),('4125','','default','Field API to add fields to entities like nodes and users.','','7.38'),('4126','','default','Overview of f